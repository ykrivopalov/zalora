-- Initial zalora-test.cabal generated by cabal init.  For further 
-- documentation, see http://haskell.org/cabal/users-guide/

name:                zalora-test
version:             0.1.0.0
synopsis:            Selection task for Zalora
-- description:         
-- license:             
license-file:        LICENSE
author:              Yuri Krivopalov
maintainer:          ykrivopalov@yandex.ru
-- copyright:           
-- category:            
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

executable zalora-test
  main-is:             Test/Zalora/Main.hs
  other-modules:       Test.Zalora.Data
                     , Test.Zalora.Log
                     , Test.Zalora.Query
                     , Test.Zalora.Storage
                     , Test.Zalora.Sitemap
                     , Test.Zalora.View
  other-extensions:    OverloadedStrings, ScopedTypeVariables, CPP, DeriveDataTypeable, FlexibleContexts, GeneralizedNewtypeDeriving, MultiParamTypeClasses, TemplateHaskell, TypeFamilies, RecordWildCards
  build-depends:       base >=4.6 && <4.7
                     , acid-state >=0.12 && <0.13
                     , aeson
                     , base64-bytestring
                     , blaze-html >=0.6 && <0.7
                     , boomerang
                     , bytestring
                     , happstack-server >=7.3 && <7.4
                     , ixset
                     , mtl >=2.1 && <2.2
                     , safecopy >=0.8 && <0.9
                     , text >=0.11 && <0.12
                     , transformers
                     , web-routes
                     , web-routes-boomerang
                     , web-routes-happstack
                     , web-routes-th
  -- hs-source-dirs:      
  default-language:    Haskell2010

executable post
  main-is:             Test/Zalora/Test/Post.hs
  other-modules:       Test.Zalora.Data
                     , Test.Zalora.Query
  other-extensions:    OverloadedStrings, ScopedTypeVariables, CPP, DeriveDataTypeable, FlexibleContexts, GeneralizedNewtypeDeriving, MultiParamTypeClasses, TemplateHaskell, TypeFamilies, RecordWildCards
  build-depends:       base >=4.6 && <4.7
                     , aeson
                     , base64-bytestring
                     , bytestring
                     , HTTP
                     , process
                     , text >=0.11 && <0.12
  -- hs-source-dirs:      
  default-language:    Haskell2010
